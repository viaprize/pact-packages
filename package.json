{
  "name": "viaprize",
  "version": "0.0.0",
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">=20.11.1"
  },
  "scripts": {
    "format-and-lint": "biome check .",
    "format-and-lint:fix": "biome check . --write",
    "clean:all": "rm -rf node_modules pnpm-lock.yaml && pnpm store prune",
    "build": "sst build",
    "dev": "sst dev",
    "dev:indexer": "cd packages/indexer && pnpm dev",
    "dev:website": "cd packages/website && pnpm dev",
    "start:indexer": "cd packages/indexer && pnpm start",
    "deploy": "sst deploy",
    "remove": "sst remove",
    "console": "sst console",
    "typecheck": "tsc --noEmit",
    "test": "sst bind -- vitest run",
    "gen": "hygen",
    "prepare": "husky",
    "sso": "aws sso login --sso-session=viaprize"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@tsconfig/node20": "^20.1.4",
    "husky": "^9.1.6",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "16.4.5",
    "sst": "3.2.28"
  }
}
